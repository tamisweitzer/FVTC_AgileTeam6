@model IEnumerable<BandZone.BL.MusicianVenueEvent>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MusicianId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BandMusicianName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VenueId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VenueName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EventTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EventHour)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.MusicianId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.BandMusicianName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.VenueId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.VenueName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.EventTime)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.EventHour)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
    </td>
</tr>
}

</table>
